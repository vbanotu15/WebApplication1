#receiving-locations {
	background: #ccc;
	position: relative;
	padding: 0 15px;
}

#receiving-locations .wrapper {
	position: absolute;
	background: #cccccc;
	margin-left: -15px;
	margin-right: -15px;
	height: 0;
	overflow: hidden;
	padding: 0 15px;
	left: 0;
}

#receiving-locations .wrapper.expanded {
	padding-bottom: 15px;
}

