.mysdi{
	background-color:#cccccc;
	padding:15px;
	width:97.5%;
}

.mysdi.lightgrey{
	background-color:#f0efef;
}

.controls-bar {
	background-color:#f0efef;
	padding: 20px;
	text-align: right;
}

.controls-bar input[type="submit"],
.controls-bar .submit {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px; /*16px ÷ 16px*/
	color:#FFFFFF!important;
	width:150px;
	height:44px;
	background-color:#424242;
	margin-left: 15px;
	border:none;
	-webkit-appearance: none;
	-moz-border-radius: 10px; /* Firefox */
	-webkit-border-radius: 10px; /* Safari, Chrome */
	border-radius:10px;
	-webkit-box-shadow: -8px 8px 15px 0 #BBBBBE;
	box-shadow: -8px 8px 15px 0 #BBBBBE;
	cursor: pointer;
}


.controls-bar input.submit:hover,
.controls-bar input.submit:active,
.controls-bar input[type="submit"]:hover,
.controls-bar input[type="submit"]:active {
	color:#424242 !important;
	background-color:#CACACA !important;
}

h4.mysdi{
	width:45%;
	color:#424242;
	float:left;
}

h5.mysdi{
	line-height:22px;
	text-align:center;
	background-color:transparent;
}

.mysdiIcons{
	float:right;
	text-align:center;
}

.mysdiIcons ul{
	float:left;
	margin-right:15px;
}

.mysdiIcons ul li{
	display:block;
}

.mysdiIcons ul li a:link, .mysdiIcons ul li a:visited, .mysdiIcons ul li a:active{
font-family: 'Montserrat', sans-serif;
font-weight: 400;
font-size:14px;
color:#424242;
text-decoration:none;
}

.mysdiIcons ul li a:hover, .mysdiIcons ul li a:focus{
	color:#ffffff;
}

.mysdiIcons ul li a.current{
	color:#ffffff;
}

.mysdiNav{
	width:95.56737588652482%; /*1078px ÷ 1128px*/
margin-right:2.3191094619666047%; /*25px ÷ 1078px*/
margin-left:2.3191094619666047%; /*25px ÷ 1078px*/
text-align:center;
}

.mysdiNav ul li{
display:inline-block;
text-align:center;
}

.mysdiNav ul li a:link, .mysdiNav ul li a:visited, .mysdiNav ul li a:active{
display: block;
font-family: 'Montserrat', sans-serif;
font-weight: 400;
font-size:14px;
color:#424242;
text-decoration:none;
background-color:#ebebeb;
padding:15px;
margin-left:5px;
margin-right:5px;
margin-bottom:10px;
border-radius: 10px 10px 0px 0px;
}

.mysdiNav ul li a:hover,.mysdiNav ul li a:focus {
	color:#fff;
	background-color:#c02e39;
}

.mysdiNav ul li a.current {
	color:#fff;
	background-color:#c02e39;
}

.Scroll{
	min-width:600px;
	overflow:auto;
}

::-webkit-scrollbar
{
  width: 14px;  /* for vertical scrollbars */
  height: 14px; /* for horizontal scrollbars */
}

::-webkit-scrollbar-track
{
  background: rgba(0, 0, 0, 0.1);
}

::-webkit-scrollbar-thumb
{
  background: rgba(0, 0, 0, 0.5);
}

